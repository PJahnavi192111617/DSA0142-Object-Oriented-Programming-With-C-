#include<iostream>
using namespace std;
class sample{
	private:
		int a,b;
	public:
		void get(void);
		void put(void);
		sample(int a1,int b1)
		{
			a=a1;
			b=b1;
		}
};
void sample::get(void)
{
	cout<<" \nThe value of a"<<a;
}	

void sample::put() 
	{
	 cout<<"\n The value of b"<<b;
		}


int main()
{
	sample smp(5,6);
	smp.get();
	smp.put();
	
}
