#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double number, squareRoot;

    cout << "Enter a positive number: ";
    cin >> number;

    if (number < 0) {
        cout << "Invalid input. Please enter a positive number." << endl;
    } else {
        squareRoot = sqrt(number);
        cout << "The square root of " << number << " is " << squareRoot << endl;
    }

    return 0;
}
