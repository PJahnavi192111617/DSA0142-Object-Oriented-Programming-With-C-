#include<iostream>
using namespace std;
class sample{
	private:
		int a,b,c;
	public:
		void get(void);
		void put(void);
		void add(void);
		sample(int a1,int b1)
		{
			a=a1;
			b=b1;
		}
};
void sample::get(void)
{
	cout<<" \nThe value of a: "<<a;
}	

void sample::put(void) 
	{
	 cout<<"\n The value of b: "<<b;
}
void sample::add(void){
	c=a+b;
	cout<<"\nThe addition of a and b is: "<<c;
}

int main()
{
	sample smp(5,6);
	smp.get();
	smp.put();
	smp.add();
	return 0;
}
