#include<iostream>
using namespace std;
int main()
{
	int rows,i,j;
	cout<<"Enter the no of rows= ";
	cin>>rows;
	for(i=0;i<rows;i++)
	{
		for(j=0;j<rows;j++)
		{
			if(i==0 || i==rows-1 || j==0 || j==rows-1)
			{
				cout<<"*";
			}
			else
			{
				cout<<" ";
			}
		}
		cout<<endl;
	}
	return 0;
}
